<div id="<%= dom_id config_question %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Program:</strong>
    <%= config_question.program_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this config question", config_question, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this config question', edit_config_question_path(config_question), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
