<div id="<%= dom_id program_manager %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Uniqname:</strong>
    <%= program_manager.uniqname %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">First name:</strong>
    <%= program_manager.first_name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Last name:</strong>
    <%= program_manager.last_name %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this program manager", program_manager, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this program manager', edit_program_manager_path(program_manager), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
