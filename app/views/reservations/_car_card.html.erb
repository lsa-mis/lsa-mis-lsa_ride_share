<%= turbo_frame_tag dom_id(car) do  %>
  <div class="flex flex-col h-full">
    <div class="m-2 mb-3 sm:px-0">
      <%= car.car_number %> - <%= car.number_of_seats %> seats
    </div>
    <div>
      <% available_ranges(car, day, unit_id).each do |range| %>
        <div>
          <%= range %>
        </div>
      <% end %>
    </div>
  </div>
<% end %>
