<%= turbo_frame_tag "end_day_long" do %>
  <%= label_tag :day_end, "Choose an End Date", class: "fancy_label" %>
  <%= text_field_tag :day_end, @day_end,
    data: {
    controller: "flatpickr",
    flatpickr_date_format: "Y-m-d",
    flatpickr_min_date: @min_date,
    flatpickr_max_date: @max_date
    }, "data-longreservation-target": "day_end", "data-action": "change->longreservation#availableCars",
    class: "input_text_field"
  %>
<% end %>
