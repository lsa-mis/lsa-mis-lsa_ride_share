<div id="<%= dom_id @config_question_program %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Program:</strong>
    <%= @config_question_program.subject.upcase %> <%= @config_question_program.catalog_number.titleize %> - <%= @config_question_program.class_section.titleize %>
  </p>
  <% @config_questions.each do |q| %>
    <div class="flex flex-cols items-center">
      <div>
        <%= button_to "Delete", program_config_question_path(@config_question_program, q), method: :delete, data: { confirm: 'Are you sure?' }, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
      </div>
        <%= link_to 'Edit', edit_program_config_question_path(@config_question_program, q), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
      <div>
      </div>
      <div>
        <%= q.question %>
      </div>
      <div>
        <%= q.answer %>
      </div>
    </div>
  <% end %>

  
</div>
