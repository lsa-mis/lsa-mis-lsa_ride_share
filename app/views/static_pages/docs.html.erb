  document.addEventListener(‘click’, function(event) {
    var target = event.target;
    if (target.tagName === ‘A’) {
      // event.preventDefault(); // Prevent the default link behavior
      var iframe = document.getElementById(‘docs’); // Replace with your iframe’s ID
      var newUrl = target.href;
      var message = {
        params: {
        }
      };
      // Send the message containing parameters to the iframe
      iframe.contentWindow.postMessage(message, newUrl);
    }
  });
 </script>
